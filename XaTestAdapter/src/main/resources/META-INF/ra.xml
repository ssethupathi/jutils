<?xml version="1.0" encoding="UTF-8"?>
<connector id="Connector_ID" version="1.5"
	xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/connector_1_5.xsd">
	<description></description>
	<display-name>XaTesterAdapter</display-name>
	<vendor-name>Temenos</vendor-name>
	<eis-type>XaTester</eis-type>
	<resourceadapter-version>0.1</resourceadapter-version>
	<resourceadapter>
		<outbound-resourceadapter>
			<connection-definition>
				<managedconnectionfactory-class>com.temenos.test.XaTestManagedConnectionFactory</managedconnectionfactory-class>
				<connectionfactory-interface>javax.resource.cci.ConnectionFactory</connectionfactory-interface>
				<connectionfactory-impl-class>com.temenos.test.XaTestConnectionFactory</connectionfactory-impl-class>
				<connection-interface>javax.resource.cci.Connection</connection-interface>
				<connection-impl-class>com.temenos.test.XaTestConnection</connection-impl-class>
			</connection-definition>
			<transaction-support>XATransaction</transaction-support>
			<reauthentication-support>false</reauthentication-support>
		</outbound-resourceadapter>
	</resourceadapter>
</connector>
